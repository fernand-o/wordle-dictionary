{
  "name": "Wordle Dictionary",
  "description": "Displays the definition of each word in the Wordle game",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://www.nytimes.com/games/wordle/index.html"
      ]
    }
  ]
}